<?xml version = "1.0" encoding = "UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping >
	<class name="modelo.fichadas.tren.EstacionTren" table="estaciontren">
		<id column="idEstacion" name="idEstacion">
			<generator class = "identity" />
		</id>
		<property column="nombre" name="nombre" type="string" />
		<many-to-one name="recorridoTren" class="modelo.fichadas.tren.RecorridoTren" fetch="select" column="idRecorrido" not-null="true" />
 	</class>
</hibernate-mapping>