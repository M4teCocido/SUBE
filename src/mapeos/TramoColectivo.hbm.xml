<?xml version = "1.0" encoding = "UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping >
	<class name="modelo.fichadas.colectivo.TramoColectivo" table="tramocolectivo">
		<id column="idTramo" name="idTramo">
			<generator class = "identity" />
		</id>
		<property column="nombre" name="nombre" type="string" />
		<property column="precio" name="precio" type="java.math.BigDecimal" />
		<many-to-one name="ramal" class="modelo.fichadas.colectivo.RamalColectivo" fetch="select" column="idRamal" not-null="true" />
	</class>
</hibernate-mapping>